generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model user {
  id        String       @id
  username  String       @unique(map: "User_username_key")
  email     String       @unique(map: "User_email_key")
  password  String
  gender    user_gender?
  createdAt DateTime     @default(now())
  updatedAt DateTime
  role      user_role?
  nomor     String?
}

enum user_gender {
  Pria
  Wanita
}

enum user_role {
  Pengelola
  Penyewa
}
